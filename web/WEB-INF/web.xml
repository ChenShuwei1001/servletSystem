<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <listener>
        <listener-class>listener.DBinit</listener-class>
    </listener>


    <!--fankehu2 add begin-->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>Admin_addmovie</servlet-name>
        <servlet-class>admin_fkh.Admin_addmovie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_addmovie</servlet-name>
        <url-pattern>/servletSystem/admin_fkh/Admin_addmovie</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Admin_LoadData_movie_list</servlet-name>
        <servlet-class>admin_fkh.Admin_LoadData_movie_list</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_LoadData_movie_list</servlet-name>
        <url-pattern>/servletSystem/admin_fkh/Admin_LoadData_movie_list</url-pattern>
    </servlet-mapping>
    <!--fankehu2 add end-->
</web-app>

